           	var question = tieArr[i][1]; //console.log('question'+question);
            if(i>0){
              for(var v=0;v<track_questions.length;v++){ //console.log(track_questions[v]);
                if(track_questions[v]==question){
                  exist = 1;
                  break;
                }
              }
            }
             
            if(exist==1){
              continue;
            }
            
            
           
            var seq = '';
            if(seq_var==0){
              seq = "A)";
            }
            if(seq_var==1){
              seq = "B)";
            }
            if(seq_var==2){
              seq = "C)";
            }
            if(seq_var==3){
              seq = "D)";
            }
            if(seq_var==4){
              seq = "E)";
            }
            if(seq_var==5){
              seq = "F)";
            }
            if(seq_var==6){
              seq = "G)";
            }
            if(seq_var==7){
              seq = "H)";
            }
            if(seq_var==8){
              seq = "I)";
            }
            if(seq_var==9){
              seq = "J)";
            }
            if(seq_var==10){
              seq = "K)";
            }
            if(seq_var==11){
              seq = "L)";
            }
            if(seq_var==12){
              seq = "M)";
            }
            if(seq_var==13){
              seq = "N)";
            }
            if(seq_var==14){
              seq = "O)";
            }
            if(seq_var==15){
              seq = "P)";
            }
            if(seq_var==16){
              seq = "Q)";
            }
            if(seq_var==17){
              seq = "R)";
            }
            if(seq_var==18){
              seq = "S)";
            }
            if(seq_var==19){
              seq = "T)";
            }
            if(seq_var==20){
              seq = "U)";
            } 
            //console.log(seq);
            
            track_questions.push(question);
            seq_var = seq_var + 1;
           console.log('qu'+question);
            var option = '';
            if(question =="Asparagus"){
            	var option = "I'll take anything tender+green topped with crispy fried egg.";
            }
            
            if(question =="Avocado"){
            	var option = "A Mexican feast with all the dips and tortilla chips.";
            }
            
            if(question =="Beets"){
            	var option = "A hearty and colorful potato + root veggie hash.";
            }
            if(question =="Broccoli"){
            	var option = "A hearty, veggie-filled casserole cooked low and slow - with lots of cheese please!";
            }
            if(question =="Butternut squash"){
            	var option = "A warming bowl of winter squash soup topped with creme fraiche and toasted hazelnuts.";
            }
            if(question =="Cabbage"){
            	var option = "A hefty side of homemade slaw (ideally alongside some killer BBQ).";
            }
            if(question =="Carrot"){
            	var option = "A colorful crudite platter full of crunchy raw veggies.";
            }
            if(question =="Cauliflower"){
            	var option = "A rich and flavorful coconut vegetable curry";
            }
            if(question =="Corn"){
            	var option = "A summer picnic with all the yummy sides and fresh squeezed lemonade.";
            }
            if(question =="Cucumber"){
            	var option = "Pickles. Give me all the pickles!";
            }
            if(question =="Eggplant"){
            	var option = "A mediterranean platter filled with hummus, babaganoush, and pita bread.";
            }
            if(question =="Fennel"){
            	var option = "An instagram-worthy salad with lots of herbs and a rainbow of thinly sliced farmers' market veggies.";
            }
            if(question =="Garlic"){
            	var option = "A thick slab of rosemary focaccia bread, slathered in olive oil.";
            }
            if(question =="Jalapeno Pepper"){
            	var option = "As long as it's spicy, I don't care!";
            }
            if(question =="Kale"){
            	var option = "A 'buddha bowl' filled with veggies, beans, greens, and a killer sauce.";
            }
            if(question =="Kohlrabi" || question =="Kholrabi"){
            	var option = "I'm all about those weird, but strangly addicting veggie chips.";
            }
            if(question =="Onion"){
            	var option = "A steaming pan of paella, bursting with a cornucopia of veggies, seafood, and spices.";
            }
            if(question =="French Breakfast Radish"){
            	var option = "A thick + toasted piece of sourdough bread topped with butter and flakey sea salt.";
            }
            if(question =="Sugar Snap Pea"){
            	var option = "A thick + toasted piece of sourdough bread topped with butter and flakey sea salt.";
            }
            if(question =="French Breakfast Radish" || question =="Breakfast Radish" || question =="Radish"){
            	var option = "A creamy bowl of risotto with heaps of parmesan, lemon, and spring veggies.";
            }
            if(question =="Swiss Chard"){
            	var option = "A morning frittata filled with heaps of greens.";
            }
            if(question =="Tomato"){
            	var option = "Pizza! Duh."; 					
            }
            if(option!=''){ console.log(tieArr[i][3]);
              
                var option_seq = seq+" "+option; 
                option = option_seq;
                html += '<div class="input_each"><input data-j1="'+tieArr[i][1]+'" data-j2="'+tieArr[i][2]+'" data-j3="'+tieArr[i][3]+'" data-j4="'+tieArr[i][4]+'" data-j5="'+tieArr[i][5]+'" onChange="tie_breaker(this)" id="breaker_'+i+'" type="radio" value="'+question+'"/><label for="breaker_'+i+'">';
                html += '<p>';
                html += option;
                html += '</p></label></div>';
            }
            
            else if(question != '' && option==''){
              
                var option_seq = seq+" "+option; 
            	option = option_seq;
             	html += '<div class="input_each"><input data-j1="'+tieArr[i][1]+'" data-j2="'+tieArr[i][2]+'" data-j3="'+tieArr[i][3]+'" data-j4="'+tieArr[i][4]+'"  data-j5="'+tieArr[i][5]+'" onChange="tie_breaker(this)" id="breaker_'+i+'" type="radio" value="'+question+'"/><label for="breaker_'+i+'">';
              	html += '<p>';
              	html += option;
              	html += '</p></label></div>';
            }
              
                
            
                                              
          
          }
          
          html += '</div>';
          $('.rte').append(html);
        }
        //if($('.resultProduct h1').html() == ''){
        else { 
          var item = resArr[Math.floor(Math.random()*resArr.length)];
          var l = item.split('|'); console.log('1. facebook social'+$('.share-facebook').attr('href'));
          console.log(tieArr[0][4]);
          var base_url = $('.share-facebook').attr('href');
          $('.share-facebook').attr('href',l[4]);
          $('.resultProduct h1').html('<a href="'+ l[4]  +'">'+ l[1] + '</a>');  
          $('.resultProduct .grid img').attr('src', l[2]);  
          $('.resultProduct .resultBody').html(l[3]); 
          $('.resultProduct .resLink a').attr('href',base_url+''+l[4]); 
          var tie_img = get_tie_img(l[1]);
          $('.tie_img').attr('src', tie_img);
          $('.resultProduct').fadeIn('slow');          
          $('.quiz_main').css('background-image', 'none'); 
          $('.prompt-link').attr('href','https://edibleschoolyard.org/');
        }
        
      }
      
    });
    
    $('.take_quiz').on('click', function(){
      $('.quiz_start').hide();
      $('.quiz_outer:first').removeClass('quiz_hide');
    });
  });
