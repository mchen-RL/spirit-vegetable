  function tie_breaker(obj){ 
    
    $('.extra-div').hide(); 
    
    var j1 = $(obj).data('j1');
    var j2 = $(obj).data('j2');
    var j3 = $(obj).data('j3');
    var j4 = $(obj).data('j4');
    //var j5 = $(obj).data('j4');
    console.log('1. facebook social'+$('.share-facebook').attr('href'));
    
    var base_url = $('.share-facebook').attr('href');
    $('.share-facebook').attr('href',base_url+''+j4);
    $('.resultProduct h1').html('<a href="'+ j4  +'">'+ j1 + '</a>');  
    $('.resultProduct .grid img').attr('src', j2);  
    $('.resultProduct .resultBody').html(j3);          
    $('.resultProduct .resLink a').attr('href', j4);
   // $('.tie_img').attr('src', j5);
    var tie_img = get_tie_img(j1);
    $('.tie_img').attr('src', tie_img);
    $('.resultProduct').fadeIn('slow');
    $('.quiz_main').css('background-image', 'none');
    $('.prompt-link').attr('href','https://edibleschoolyard.org/');
    
    
   
  }
</script>

